    <!-- CashAccount -->
    <entity entity-name="CashAccount"
            package-name="org.apache.ofbiz.accounting.ledger"
            title="Cash Account Entity">
        <field name="cashAccountId" type="id"></field>
        <field name="statusId" type="id"></field>
        <field name="cashAccountClassId" type="id"></field>
        <field name="parentCashAccountId" type="id"></field>
        <field name="accountCode" type="name"></field>
        <field name="accountName" type="name"></field>
        <field name="description" type="description"></field>
        <field name="partyId" type="id"></field>
        <field name="externalId" type="id"><description>id of the account in an external system where the accounts are imported/exported</description></field>
        <prim-key field="cashAccountId"/>
        <relation type="one" fk-name="CSHACCT_CLSS" rel-entity-name="CashAccountClass">
            <key-map field-name="cashAccountClassId"/>
        </relation>
        <relation type="one" fk-name="CSHACCT_PAR" title="Parent" rel-entity-name="CashAccount">
            <key-map field-name="parentCashAccountId" rel-field-name="cashAccountId"/>
        </relation>
        <relation type="one" fk-name="CSHACCT_PTY" rel-entity-name="Party">
            <key-map field-name="partyId" />
        </relation>
        <relation type="one" fk-name="CSHACCT_STS" rel-entity-name="StatusItem">
            <key-map field-name="statusId" />
        </relation>
        <index name="CSHACCT_UNQCD" unique="true">
            <index-field name="accountCode"/>
        </index>
    </entity>

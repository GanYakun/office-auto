    <entity entity-name="FixedAssetFault" package-name="com.banfftech.bfdemo" title="FixedAsset Fault Entity">
        <field name="fixedAssetFaultId" type="id"></field>
        <field name="fixedAssetId" type="id"></field>
        <field name="productFaultId" type="id"></field>
        <field name="faultEnumId" type="id"></field>
        <field name="productPartsId" type="id">
            <description>故障部位</description>
        </field>
        <field name="statusId" type="id"></field>
        <field name="description" type="description"></field>
<!--        <field name="custRequestId" type="id"></field>-->
<!--        <field name="custRequestItemSeqId" type="id"></field>-->
        <field name="workEffortId" type="id"></field>
        <field name="closedDate" type="date-time"></field>
        <field name="comment" type="comment"></field>
        <field name="solutionText" type="very-long"></field>
        <field name="isReplace" type="indicator"></field>
        <prim-key field="fixedAssetFaultId"/>
        <relation type="one" fk-name="FXDASTFAULT_FIXASST" rel-entity-name="FixedAsset">
            <key-map field-name="fixedAssetId"/>
        </relation>
        <relation type="one" fk-name="FXDASTFAULT_PRDFAULT" rel-entity-name="ProductFault">
            <key-map field-name="productFaultId"/>
        </relation>
        <relation type="one" fk-name="FXDASTFAULT_WE" rel-entity-name="WorkEffort">
            <key-map field-name="workEffortId"/>
        </relation>
        <relation type="one" fk-name="FXDASTFAULT_SI" rel-entity-name="StatusItem">
            <key-map field-name="statusId"/>
        </relation>
        <relation type="many" rel-entity-name="WarrantyRec">
            <key-map field-name="fixedAssetFaultId"/>
        </relation>
        <relation type="one" fk-name="FXDASTFAULT_ENUM" rel-entity-name="Enumeration">
            <key-map field-name="faultEnumId" rel-field-name="enumId"/>
        </relation>
        <relation type="one" fk-name="FXDASTFAULT_PRDPARTS" rel-entity-name="ProductParts">
            <key-map field-name="productPartsId"/>
        </relation>
    </entity>


    <entity entity-name="UserLoginAlias"
            package-name="com.dpbird.basecamp"
            never-cache="true"
            title="用户登陆别名">
        <field name="webSiteId" type="id">
            <description>app</description>
        </field>
        <field name="userLoginAliasId" type="id-vlong">
            <description>用户别名</description>
        </field>
        <field name="userLoginAliasTypeId" type="id">
            <description>别名类型</description>
        </field>
        <field name="enable" type="indicator">
            <description>是否开启</description>
        </field>
        <field name="userLoginId" type="id-vlong">
            <description>用户名</description>
        </field>
        <prim-key field="userLoginAliasId"/>
        <prim-key field="webSiteId"/>
        <relation type="one" fk-name="ULA_ULAT" rel-entity-name="UserLoginAliasType">
            <key-map field-name="userLoginAliasTypeId"/>
        </relation>
        <relation type="one" fk-name="ULA_WS" rel-entity-name="WebSite">
            <key-map field-name="webSiteId"/>
        </relation>
    </entity>


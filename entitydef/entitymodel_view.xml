    <view-entity entity-name="CustRequestAndNote"
                 package-name="org.apache.ofbiz.order.request"
                 title="Customer Request And Note">
        <member-entity entity-alias="CRN" entity-name="CustRequestNote"/>
        <member-entity entity-alias="ND" entity-name="NoteData"/>
        <alias-all entity-alias="CRN">
            <exclude field="noteId"/>
        </alias-all>
        <alias-all entity-alias="ND"/>
        <view-link entity-alias="CRN" rel-entity-alias="ND">
            <key-map field-name="noteId"/>
        </view-link>
        <relation type="one-nofk" rel-entity-name="CustRequest">
            <key-map field-name="custRequestId"/>
        </relation>
    </view-entity>

    <!-- 产品存量总计 -->
    <view-entity entity-name="ProductQuantitySummary" package-name="org.apache.ofbiz.accounting.reports">
        <member-entity entity-alias="II" entity-name="InventoryItem"/>
        <member-entity entity-alias="F" entity-name="Facility"/>
        <member-entity entity-alias="P" entity-name="Product"/>
        <alias entity-alias="II" name="productId" group-by="true"/>
        <alias entity-alias="P" name="productTypeId" group-by="false"/>
        <alias entity-alias="II" name="facilityId" group-by="true"/>
        <alias entity-alias="II" name="quantityOnHandTotal" field="quantityOnHandTotal" function="sum"/>
        <alias entity-alias="II" name="availableToPromiseTotal" field="availableToPromiseTotal" function="sum"/>

        <view-link entity-alias="II" rel-entity-alias="F">
            <key-map field-name="facilityId" rel-field-name="facilityId"/>
        </view-link>

        <view-link entity-alias="II" rel-entity-alias="P">
            <key-map field-name="productId" rel-field-name="productId"/>
        </view-link>

        <relation type="many" rel-entity-name="InventoryItem">
            <key-map field-name="facilityId"/>
            <key-map field-name="productId"/>
        </relation>
        <relation type="many" rel-entity-name="Product">
            <key-map field-name="productId"/>
        </relation>

    </view-entity>


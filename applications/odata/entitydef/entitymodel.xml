    <!--    以下为sap ui5 fe的SelectionVariant和PresentationVariant的存储方案-->
    <entity entity-name="VariantFile"
            package-name="com.dpbird.odata"
            title="Variant File">
        <field name="userLoginId" type="id"/>
        <field name="fileName" type="id"/>
        <field name="fileType" type="id"/>
        <field name="variantManagementReference" type="id-vlong"/>
        <field name="variantReference" type="id-vlong"/>
        <field name="reference" type="id-vlong"/>
        <field name="packageName" type="name"/>
        <field name="variantData" type="very-long"/>
        <prim-key field="fileName"/>
        <prim-key field="userLoginId"/>
        <relation type="one" fk-name="VF_UL" rel-entity-name="UserLogin">
            <key-map field-name="userLoginId"/>
        </relation>
    </entity>

    <entity entity-name="EdmConfig"
            package-name="com.dpbird.odata"
            title="Edm Config">
        <field name="edmConfigId" type="id"/>
        <field name="edmConfigName" type="id"/>
        <field name="edmConfigVersion" type="value"/>
        <field name="edmConfigContent" type="very-long"/>
        <prim-key field="edmConfigId"/>
    </entity>

    <entity entity-name="OdataApp" package-name="com.dpbird.odata">
        <field name="appId" type="id"/>
        <field name="appName" type="name"/>
        <field name="description" type="description"/>
        <prim-key field="appId"/>
    </entity>

    <entity entity-name="Tiles" package-name="com.dpbird.odata">
        <field name="tilesId" type="id"/>
        <field name="tilesName" type="id"/>
        <field name="description" type="description"/>
        <prim-key field="tilesId"/>
    </entity>

    <entity entity-name="OdataAppPermission"
            package-name="com.dpbird.odata"
            title="Odata service permissions">
        <field name="appId" type="id"/>
        <field name="permissionId" type="id"/>
        <prim-key field="appId"/>
        <prim-key field="permissionId"/>
        <relation type="one" rel-entity-name="OdataApp">
            <key-map field-name="appId"/>
        </relation>
        <relation type="one" rel-entity-name="SecurityPermission">
            <key-map field-name="permissionId"/>
        </relation>
    </entity>

    <entity entity-name="TilesPermission"
            package-name="com.dpbird.odata"
            title="Tiles permissions">
        <field name="tilesId" type="id"/>
        <field name="permissionId" type="id"/>
        <prim-key field="tilesId"/>
        <prim-key field="permissionId"/>
        <relation type="one" rel-entity-name="Tiles">
            <key-map field-name="tilesId"/>
        </relation>
        <relation type="one" fk-name="TILES_PERMISSION_SECURITY" rel-entity-name="SecurityPermission">
            <key-map field-name="permissionId"/>
        </relation>
    </entity>

    <entity entity-name="Internationalization"
            package-name="com.dpbird.odata"
            title="International translation">
        <description>储存国际化翻译</description>
        <field name="id" type="id"/>
        <field name="lang" type="id"/>
        <field name="property" type="value"/>
        <field name="translation" type="value"/>
        <prim-key field="id"/>
    </entity>

    <entity entity-name="ProcessEntity" package-name="com.banfftech.odata">
        <description>业务对象</description>
        <field name="processEntityId" type="id"><description>主键</description></field>
        <field name="processEntityName" type="name"><description>ofbiz实体名称</description></field>
        <field name="processEntityTypeId" type="name"><description>实体类型标识</description></field>
        <field name="isOpen" type="indicator"><description>是否开放</description></field>
        <field name="description" type="value"><description>实体描述</description></field>
        <prim-key field="processEntityId"/>
        <relation type="many" fk-name="PROC_EN_FD" rel-entity-name="ProcessField">
            <key-map field-name="processEntityId"/>
        </relation>
    </entity>

    <entity entity-name="ProcessField" package-name="com.banfftech.odata">
        <description>业务对象字段</description>
        <field name="processFieldId" type="id"><description>主键</description></field>
        <field name="processFieldName" type="name"><description>字段名称</description></field>
        <field name="processFieldTypeId" type="id"><description>字段类型</description></field>
        <field name="valueTypeId" type="id"><description>选择对象的类型条件</description></field>
        <field name="asCondition" type="indicator"><description>是否可以在审理流程中当作条件</description></field>
        <field name="isOpen" type="indicator"><description>是否开放</description></field>
        <field name="description" type="value"><description>字段描述</description></field>
        <field name="processEntityId" type="id"><description>对应实体外键</description></field>
        <prim-key field="processFieldId"/>
        <relation type="one" fk-name="PROC_FD_TYPE" rel-entity-name="ProcessFieldType">
            <key-map field-name="processFieldTypeId"/>
        </relation>
    </entity>

    <entity entity-name="ProcessFieldLabel" package-name="com.banfftech.odata">
        <description>业务对象字段标签</description>
        <field name="processFieldLabelId" type="id"><description>主键</description></field>
        <field name="processFieldId" type="id"><description>对应字段主键</description></field>
        <field name="language" type="id"><description>语言</description></field>
        <field name="value" type="value"><description>译文</description></field>
        <prim-key field="processFieldLabelId"/>
        <relation type="one" fk-name="PROC_FD_LABEL" rel-entity-name="ProcessField">
            <key-map field-name="processFieldId"/>
        </relation>
    </entity>

    <entity entity-name="ProcessFieldType" package-name="com.banfftech.odata">
        <description>业务对象字段类型</description>
        <field name="processFieldTypeId" type="id"><description>类型标识</description></field>
        <field name="description" type="value"><description>描述</description></field>
        <prim-key field="processFieldTypeId"/>
    </entity>

    <!-- 存储ofbiz数据结构 -->
    <entity entity-name="OfbizEntity" package-name="com.banfftech.odata">
        <description>记录所有的实体</description>
        <field name="ofbizEntityId" type="id"><description>ID</description></field>
        <field name="ofbizEntityName" type="name"><description>实体名称</description></field>
        <field name="description" type="description"><description>描述</description></field>
        <prim-key field="ofbizEntityId"/>
    </entity>

    <entity entity-name="OfbizField" package-name="com.banfftech.odata">
        <description>记录实体字段</description>
        <field name="ofbizFieldId" type="id"><description>ID</description></field>
        <field name="ofbizEntityId" type="id"><description>实体ID</description></field>
        <field name="ofbizFieldName" type="name"><description>字段名称</description></field>
        <field name="isPrimaryKey" type="indicator"><description>是否是主键</description></field>
        <field name="description" type="description"><description>描述</description></field>
        <prim-key field="ofbizFieldId"/>
        <relation type="one" rel-entity-name="OfbizEntity" fk-name="OFBIZ_FIELD_EN">
            <key-map field-name="ofbizEntityId"/>
        </relation>
    </entity>

    <entity entity-name="OfbizRelation" package-name="com.banfftech.odata">
        <description>记录所有的实体</description>
        <field name="ofbizRelationId" type="id"><description>ID</description></field>
        <field name="ofbizEntityId" type="id"><description>实体ID</description></field>
        <field name="ofbizRelationTitle" type="name"><description>标题</description></field>
        <field name="ofbizRelationEntity" type="name"><description>关联实体名称</description></field>
        <field name="ofbizRelationName" type="name"><description>组合名称</description></field>
        <field name="ofbizRelationType" type="value"><description>关联类型</description></field>
        <prim-key field="ofbizRelationId"/>
        <relation type="one" rel-entity-name="OfbizEntity" fk-name="OFBIZ_REL_EN">
            <key-map field-name="ofbizEntityId"/>
        </relation>
    </entity>

</entitymodel>
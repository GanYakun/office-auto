	<extend-entity entity-name="Person">
		<relation type="many" title="From" rel-entity-name="PartyRelationship">
			<key-map field-name="partyId" rel-field-name="partyIdFrom"/>
		</relation>
		<relation type="many" title="To" rel-entity-name="PartyRelationship">
			<key-map field-name="partyId" rel-field-name="partyIdTo"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="PartyRole">
		<relation type="one-nofk" rel-entity-name="PartyGroup">
			<key-map field-name="partyId"/>
		</relation>
	</extend-entity>
	<extend-entity entity-name="Party">
		<relation type="many" title="From" rel-entity-name="PartyRelationshipAndContactMechDetail">
			<key-map field-name="partyId" rel-field-name="partyIdFrom"/>
		</relation>
		<relation type="many" rel-entity-name="CashAccount">
			<key-map field-name="partyId"/>
		</relation>
		<relation type="many" rel-entity-name="Budget">
			<key-map field-name="partyId"/>
		</relation>
		<relation type="many" rel-entity-name="PartySurveyAppl">
			<key-map field-name="partyId"/>
		</relation>
	</extend-entity>
	<extend-entity entity-name="OrderItemShipGroup">
		<relation type="many"  rel-entity-name="OrderShipment">
			<key-map field-name="orderId" />
			<key-map field-name="shipGroupSeqId" />
		</relation>
	</extend-entity>

	<extend-entity entity-name="Person">
		<field name="personImageUrl" type="url"/>
		<relation type="many" rel-entity-name="PartyRole">
			<key-map field-name="partyId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="SupplierProduct">
		<relation type="one-nofk" rel-entity-name="PartyGroup">
			<key-map field-name="partyId"/>
		</relation>
		<relation type="one-nofk" rel-entity-name="SupplierParty">
			<key-map field-name="partyId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="ReturnHeader">
		<relation type="many" rel-entity-name="ReturnHeaderNote">
			<key-map field-name="returnId" rel-field-name="returnId"/>
		</relation>
	</extend-entity>


	<extend-entity entity-name="CustRequest">
		<relation type="many"  rel-entity-name="CustRequestAndNote">
			<key-map field-name="custRequestId"/>
		</relation>
		<relation type="one-nofk"  rel-entity-name="PartyAndContact">
			<key-map field-name="fromPartyId" rel-field-name="partyId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="Quote">
		<field name="custRequestId" type="id"/>
		<relation type="one"  rel-entity-name="CustRequest">
			<key-map field-name="custRequestId" rel-field-name="custRequestId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="OrderHeader">
		<field name="quoteId" type="id"/>
		<relation type="one"  rel-entity-name="Quote">
			<key-map field-name="quoteId" rel-field-name="quoteId"/>
		</relation>
		<relation type="many"  rel-entity-name="WorkEffort">
			<key-map field-name="orderId" rel-field-name="orderId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="Invoice">
		<field name="orderId" type="id"/>
		<relation type="one"  rel-entity-name="OrderHeader">
			<key-map field-name="orderId" rel-field-name="orderId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="CustRequestItem">
		<relation type="one"  fk-name="CUST_REQITM_PRODUCT" rel-entity-name="Product">
			<key-map field-name="productId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="Shipment">
		<field name="trackingIdNumber" type="id"></field>
		<relation type="one" rel-entity-name="PartyUserLogin">
			<key-map field-name="createdByUserLogin" rel-field-name="userLoginId"/>
		</relation>
		<relation type="one" title="From" rel-entity-name="PartyGroupAndContact">
			<key-map field-name="partyIdFrom" rel-field-name="partyId"/>
		</relation>
		<relation type="one" title="To" rel-entity-name="PartyGroupAndContact">
			<key-map field-name="partyIdTo" rel-field-name="partyId"/>
		</relation>
	</extend-entity>


	<extend-entity entity-name="WorkEffort">
		<field name="partyId" type="id"/>
		<relation type="one" title="Priority" rel-entity-name="Enumeration">
			<key-map field-name="priority" rel-field-name="enumId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="Invoice">
		<relation type="one-nofk" title="From" rel-entity-name="PartyAndContact">
			<key-map field-name="partyIdFrom" rel-field-name="partyId"/>
		</relation>
		<relation type="one-nofk" rel-entity-name="PartyAndContact">
			<key-map field-name="partyId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="OrderHeader">
		<field name="agreementId" type="id"></field>
		<relation type="one" rel-entity-name="Agreement">
			<key-map field-name="agreementId" rel-field-name="agreementId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="PartySurveyAppl">
		<relation type="many" rel-entity-name="SurveyQuestionAppl">
			<key-map field-name="surveyId" rel-field-name="surveyId"/>
		</relation>
		<relation type="one" rel-entity-name="WorkEffortAndPartyGroupContact">
			<key-map field-name="partyId" rel-field-name="partyId"/>
		</relation>
		<relation type="one" rel-entity-name="PartyGroupAndContact">
			<key-map field-name="partyId" rel-field-name="partyId"/>
		</relation>
	</extend-entity>
	
	<extend-entity entity-name="SurveyQuestion">
		<relation type="one-nofk" rel-entity-name="SurveyQuestionAnswer">
			<key-map field-name="surveyQuestionId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="WorkFlowMember">
		<relation type="one-nofk" rel-entity-name="WorkEffortAndPartyGroupContact">
			<key-map field-name="memberEntityId" rel-field-name="workEffortId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="WorkEffortContent">
		<relation type="one-nofk" rel-entity-name="ContentAndMediaDataResource">
			<key-map field-name="contentId" rel-field-name="contentId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="ContactMechPurposeType">
		<field name="parentTypeId" type="id"/>
		<relation type="one-nofk" title="Parent" rel-entity-name="ContactMechPurposeType">
			<key-map field-name="parentTypeId" rel-field-name="contactMechPurposeTypeId"/>
		</relation>
	</extend-entity>

	<extend-entity entity-name="PostalAddress">
		<field name="areaCode" type="very-short"/>
		<field name="contactNumber" type="short-varchar"/>
		<field name="email" type="short-varchar"/>
	</extend-entity>

</entitymodel>

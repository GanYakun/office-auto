	<extend-entity entity-name="Product">
		<field name="primaryImageContentId" type="id"><description>o3 define 用于列表图片</description></field>
		<relation type="one" fk-name="PROD_CONTENT" rel-entity-name="Content">
			<key-map field-name="primaryImageContentId" rel-field-name="contentId"/>
		</relation>
        <relation type="many" fk-name="PROD_ITQTY_SUM" rel-entity-name="ProductQuantitySummary">
            <key-map field-name="productId"/>
        </relation>
		<relation type="one-nofk" rel-entity-name="UserLoginAndPersonDetail">
			<key-map field-name="createdByUserLogin" rel-field-name="userLoginId"/>
		</relation>
		<relation type="one-nofk" rel-entity-name="UserLoginAndPersonDetail">
			<key-map field-name="lastModifiedByUserLogin" rel-field-name="userLoginId"/>
		</relation>
		<relation type="many" rel-entity-name="ProductFeatureAndCategory">
			<key-map field-name="primaryProductCategoryId" rel-field-name="productCategoryId"/>
		</relation>
		<relation type="many" rel-entity-name="ProductFeatureAndCatAppl">
			<key-map field-name="primaryProductCategoryId" rel-field-name="productCategoryId"/>
		</relation>
		<relation type="many" rel-entity-name="ProductAllowedFeatureType">
			<key-map field-name="primaryProductCategoryId" rel-field-name="productCategoryId"/>
			<key-map field-name="productId" rel-field-name="productId"/>
		</relation>
		<relation type="many" rel-entity-name="FacilityAllInventory">
			<key-map field-name="productId" rel-field-name="productId"/>
		</relation>
    </extend-entity>

